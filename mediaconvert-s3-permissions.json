{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::chime-recordings-live-meeting-app",
        "arn:aws:s3:::chime-recordings-live-meeting-app/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::chime-recordings-live-meeting-app/*"
      ]
    }
  ]
}
